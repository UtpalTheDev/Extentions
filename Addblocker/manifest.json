{
    "manifest_version":2,
    "name":"Adblock Running",
    "version":"1.0",

    "description":"Add blocker.",

    "icons":{
        "48":"/addblock2.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
      ],
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ],

    "background": {
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"addblock2.png"
       
    }

}